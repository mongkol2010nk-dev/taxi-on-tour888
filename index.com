<script>
function animateCounter(counter) {
  const target = +counter.getAttribute("data-target");
  const duration = 2000; // ‡πÄ‡∏ß‡∏•‡∏≤ 2 ‡∏ß‡∏¥
  const increment = target / (duration / 16); // ‡πÅ‡∏ö‡πà‡∏á‡∏ï‡∏≤‡∏°‡πÄ‡∏ü‡∏£‡∏°

  let current = 0;

  function update() {
    current += increment;
    if (current < target) {
      counter.innerText = Math.floor(current);
      requestAnimationFrame(update);
    } else {
      counter.innerText = target.toLocaleString(); // ‡πÅ‡∏™‡∏î‡∏á‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏û‡∏£‡πâ‡∏≠‡∏° ,
      if (target >= 1000) counter.innerText += "+"; // ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏´‡∏°‡∏∑‡πà‡∏ô‡πÅ‡∏™‡∏î‡∏á "+"
      else counter.innerText += "+";
    }
  }

  update();
}

// ‡∏™‡∏±‡∏á‡πÄ‡∏Å‡∏ï‡πÄ‡∏°‡∏∑‡πà‡∏≠ scroll ‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏Ñ‡πà‡∏≠‡∏¢‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
const counters = document.querySelectorAll(".counter");
const options = { threshold: 0.6 };
const observer = new IntersectionObserver((entries, obs) => {
  entries.forEach(entry => {
    if (entry.isIntersecting) {
      animateCounter(entry.target);
      obs.unobserve(entry.target); // ‡∏ó‡∏≥‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß
    }
  });
}, options);

counters.forEach(counter => observer.observe(counter));
</script>

